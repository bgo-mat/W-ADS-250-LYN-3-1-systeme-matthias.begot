{% extends 'base.html.twig' %}

{% block title %}Create new Categorie{% endblock %}

{% block body %}
    <h1>Create new Categorie</h1>

    {{ form_start(form) }}
    {{ form_row(form.name) }}
    <div class="question-collection">
        {{ form_row(form.questions) }}
    </div>
    <button class="btn">{{ button_label|default('Save') }}</button>
    {{ form_end(form) }}

    <a href="{{ path('app_categorie_index') }}">back to list</a>
{% endblock %}
